bool isAnagram(string s, string t) {
        unordered_map<char,int>count;
        //count the frequency of character in string s
        for(auto x:s){
            count[x]++;
        }
    //decrement the frequency of character in string t
    for(auto x:t){
        count[x]--;
    }
    //check if any character has non-zero frequency
    for(auto x:count){
        if(x.second!=0){
            return false;
        }
    }
    return true;
    }
